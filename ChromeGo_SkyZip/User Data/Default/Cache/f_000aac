function OpenWindowx(theURL) { //v2.0
  window.open(theURL,'Name','width=700, height=520,scrollbars=yes');
}

function GiveEva(authoruid, boardName, ParentDocId, DocId){
	var bEv = window.confirm('您確定給要給予這位作者一個「讚」嗎 ?');
	if (bEv) {
		formEv.authoruid.value = authoruid;
		formEv.boardName.value = boardName;
		formEv.ParentDocId.value = ParentDocId;
		formEv.DocId.value = DocId;
		formEv.submit();
	}	
}

function GiveMark(mode, authoruid, boardName, ParentDocId, DocId, id){	
	var bEv;
	switch (mode) {
	case '1':
		bEv = window.confirm('將此篇回應設為最佳解答 確定嗎 ? ');
		break;
	case '2':
		bEv = window.confirm('這則回應很有道理 確定嗎 ? ');		
		break;					
	case '3':
		bEv = window.confirm('您將全部回饋點數給這篇回答 確定嗎 ? \n\n請注意 : 您一但選擇「確定」後就不可更改!!');		
		break;
	case '4':
		bEv = window.confirm('您要將回饋點數分配給一篇以上回答 確定嗎 ? \n請按「確定」來進入下一個畫面作設定');
		formEv.action = "/pc2020v5/forum/showsametitleN.asp"
		break;
	case '0':
		bEv = window.confirm('確定取消您對這則回應的評語嗎 ?');		
		break;
	}		 	
	if (bEv) {
		formEv.authoruid.value = authoruid;
		formEv.boardName.value = boardName;
		formEv.ParentDocId.value = ParentDocId;
		formEv.DocId.value = DocId;
		formEv.id.value = id;
		formEv.board_pc2020.value = 'directx';
		formEv.mode.value = mode;		
		formEv.submit();
	}
}

function ChangeCSS(sty){
	var bC = window.confirm("確定更改字型大小 ?");
	if (bC){
			location.href = "/pc2020v5/forum/showsametitleN.asp?board_pc2020=directx&id=7515&CSS=" + sty;
	}
}


//-->
