	<tr>
		<td align=Right class=C2 colspan=2>


		[ <a href="javascript:GiveEva('gmailjoey', 'directx', '7515', '7520', '');"><font color="BLUE">給個讚</font></a> ]&nbsp;
		
		[ <a href="javascript:GiveEva('gmailjoey', 'directx', '7515', '7520', '');"><font color="BLUE">給個讚</font></a> ]&nbsp;
		
		[ <a href=/pc2020v5/forum/replyN.asp?board_pc2020=directx&id=7520><font color="#000000">回應本文</a> ]&nbsp;
		
		[ <a href="/pc2020v5/forum/postform.asp?board_pc2020=directx" target="_top"><B><font color="#000000">發表新文</b></a> ]&nbsp;							        
			
		

		[ <a href="javascript:history.go(-1);" class=cx><font color="#000000">回上頁</a> ]
		[ <a href="/forum/directx.html" class=cx><font color="#000000">回討論區列表</a> ]
